    "@vuepress/plugin-git": "^2.0.0-beta.37",
    "@vuepress/plugin-google-analytics": "^2.0.0-beta.37",
    "@vuepress/plugin-palette": "^2.0.0-beta.37",
    "@vuepress/plugin-pwa": "^2.0.0-beta.37",
    "@vuepress/plugin-pwa-popup": "^2.0.0-beta.37",
    "@vuepress/plugin-register-components": "^2.0.0-beta.37",
    "@vuepress/plugin-search": "^2.0.0-beta.37",
    "anywhere": "^1.5.0",
    "autoprefixer": "^10.4.4",
    "cpx2": "^4.2.0",
    "cssnano": "^5.1.5",
    "cz-git": "^1.2.2",
    "eslint": "8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-vuepress-typescript": "^2.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "8.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "lint-staged": "^12.3.7",
